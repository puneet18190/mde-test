<% @audios.each do |audio| %>
  $('#audio_editor_gallery_container #audio_gallery_content .jspPane').append('<%= j(render(:partial => "galleries/thumbs/audio", :locals => {:audio => audio, :index => 1})) %>');
  $('#gallery_audio_<%= audio.id %> ._select_audio_from_gallery').data('title', '<%= j audio.title %>');
<% end %>
$('#audio_editor_gallery_container').data('page', <%= @page %>);
$('._select_audio_from_gallery').addClass('_add_audio_component_to_audio_editor');
